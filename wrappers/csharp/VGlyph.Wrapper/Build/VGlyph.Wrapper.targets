<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <ItemGroup Condition=" '$(Platform)' == 'AnyCPU' ">
        <Content Include="$(MSBuildThisFileDirectory)..\runtimes\win-x64\native\vglyph.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <Link>x64\vglyph.dll</Link>
        </Content>
        <Content Include="$(MSBuildThisFileDirectory)..\runtimes\win-x86\native\vglyph.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <Link>x86\vglyph.dll</Link>
        </Content>
    </ItemGroup>
    <ItemGroup Condition=" '$(Platform)' == 'x86' ">
        <Content Include="$(MSBuildThisFileDirectory)..\runtimes\win-x86\native\vglyph.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <Link>vglyph.dll</Link>
        </Content>
    </ItemGroup>
    <ItemGroup Condition=" '$(Platform)' == 'x64' ">
        <Content Include="$(MSBuildThisFileDirectory)..\runtimes\win-x64\native\vglyph.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <Link>vglyph.dll</Link>
        </Content>
    </ItemGroup>
</Project>
